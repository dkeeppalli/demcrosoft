					<a href="#!/profile/<%= ccl.utils.encodePredefinedChars(topicObj.inspired_by.topic_owner_name)%>" id="<%= topicObj.inspired_by.topic_owner_id%>">
						<%= topicObj.inspired_by.topic_owner_name%>
					</a>
