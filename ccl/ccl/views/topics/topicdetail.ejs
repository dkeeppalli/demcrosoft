								<% }%>
							</div>
                    		<div id="commentsPageContainer"></div>
                    	</div>
                    	
                    <%}%>
                </div>
            </div>	
		<!-- Main Content Area End -->
        <!-- Right Side Content Area Start -->
            <div id="rightContent" class="span-6 rightContent last">
				
				<div class="topicUserData">
