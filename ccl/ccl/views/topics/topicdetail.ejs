<% 
	var topicObj = topicDetails;
	var tagsObj = null;
	var tagsCount = topicObj.tags.length;
	var gradeObj = null;
	var gradesCount = topicObj.grades.length;
	var subjectObj = null;
	var subjectsCount = topicObj.subjects.length;
