<div class="span-20 topicLayoutContaniner" id="topicListingPage">
<%
	var browseOptions = browseData;
	if(browseOptions.browseBy == "downloaded" || browseOptions.browseBy == "shared" || browseOptions.browseBy == "rated"){
%>
	<h3 class="left margin0"><%= ccl.utils.fullTitles(browseOptions.browseBy) %></h3> <div class="left" id="totalTopicsShow" style="display:none;"><span id="browseTopicsCount" class="browseTopicsCount"></span></div>
<%}else{%>
	<div class="margin-bottom10">
		<a href="#!"><%= ccl.bundle.label.home%></a> &gt; <a href="#<%=browseOptions.browseBy%>"><%= ccl.bundle.label.browseTopicsBy%> <%= ccl.utils.fullTitles(browseOptions.browseBy) %></a>
	</div>
	<h3 class="left margin0"><%= browseOptions.browseFor%> </h3> <div class="left" id="totalTopicsShow" class="totalTopicsShow" style="display:none;"><span id="browseTopicsCount"></span></div>
<%}%>
	<p class="clear" />
</div>
